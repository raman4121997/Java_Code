class Overload
{
    void show()
    {
      System.out.println("Method Overloading\n ");  
    }
    void show(int l, int b)
    {
        System.out.println("Area of rectangle: "+(l*b));
    }
    void show(int r)
    {
      System.out.println("\n Area of Circle: "+(3.14*r*r));  
    }
}

public class Main
{
	public static void main(String[] args)
	{
	    
	   Overload obj=new Overload();
	   obj.show();
	   obj.show(12,67);
	   obj.show(5);
	}
    
}
